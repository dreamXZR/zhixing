@import '/style/join.wxss';
page{  
  font-size: 32rpx;
  background-color: #f0f6f6;
  width: 100%;
  height: 100%;  
}
.content {
  margin: 50rpx auto;
  height: 100%;
  width: 90%;
  background-color: white;
}
.form{
  padding: 40rpx 15rpx 0 15rpx;  
  color: #b6c1c5;  
}
.checkbox_font{
  font-size: 50rpx;
}
.form .view{ 
  color: #757a7b;
  margin-left: 20px;
} 
.form .view_money{  
  display: inline-block;  
  width: 350rpx;  
  margin-left: 20px;
}  
.view1{  
  margin-bottom: 20rpx;   
}  

.picker{
    margin-left: 20px;
}
.duoxuan checkbox-group{
   margin-left: 20px;
}

.xiahuaxian{  
  width: 100%;  
  height: 3px;  
  background: #0099CC;  
  margin-top: 100rpx;  
}  
button{  
  width: 320rpx;  
  height: 60rpx;   
  line-height: 60rpx;  
  margin-top: 50rpx;  
  margin-bottom: 40rpx;  
  background: #50a5fc;  
  color: white  
}  

checkbox{
   zoom:60%;
}
.fengexian{
  margin-top: 20rpx;
  border-bottom: 1px dashed #0099CC;
}
.picker text{
  color: #757a7b;
}

.enroll{
  display: inline-block;
  float: right;
  border: 1rpx #000 solid;
  border-radius:20rpx;
  padding: 0 10rpx;
  margin-right:20rpx;
}