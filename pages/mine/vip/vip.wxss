.erweima{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}
.title{
    text-align: center;
    display: flex;
    flex-wrap:wrap;
    justify-content:center;
     align-items:center;
     padding: 40rpx 0;
} 
.title text{
    display: block;
}
.title text:last-child{
    color: #a9a9a9;
    font-size: 35rpx;
}
.money-list  {
    padding: 0 45rpx;
    display: flex;
    flex-wrap:wrap;
    justify-content:space-between;
    align-items:center;
}
.release{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    height: 120rpx;
    border-top:1px solid #b2b2b2;
    background:#4395FF;
    align-items: center;
    font-size:40rpx;
    color: #fff;
    justify-content: center;
    border-radius: 0rpx;
}
.title-top {
    padding-bottom: 15rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.title-top image {
    height: 200rpx;
    width: 200rpx;
    border-radius: 50%;
}
.prompt-title {
  margin-top: 30rpx;
    display: flex;
   padding-left: 30px;
}
.prompt-title text{
    font-size: 30rpx;
    line-height: 45rpx;

}
.prompt-title text:first-child{
    color: #d92a2a;
}
.tui-number-group{
 display: table;
 table-layout: fixed;
 width: 300rpx;
 text-align: center;
 border-radius: 6px;
 border: 1px solid #bbb;
 overflow: hidden;
}
.tui-number-cell{
 display: table-cell;
 line-height: 1.7;
 border-radius: 0;
}
button::after{
 border-bottom: none;
 border-top: none;
 border-radius: 0;
}
.money-list{
    display: flex; 
    justify-content: center;
    align-items: center;
}
.total-money{
  text-align: center;
}

.vip{
  margin-left: 20rpx;
}

.buyvip{
  display: flex;
  flex-wrap: wrap;
}
.vip1{
margin: 10rpx;
border: 1px solid #9fafae;
width: 330rpx;
flex-wrap: wrap;
text-align: center;
position: relative;
 height: 180rpx; 
}
.vipprice{
position: absolute;
top:50%;
transform: translate(0, -50%);
left: 80rpx;
color: 4395FF;
}
.vipprice .rmb{
  color: 4395FF;
  font-size: 16px;
  
}
.vipprice text{
  font-size: 25px;
