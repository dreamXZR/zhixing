  margin-top: 20rpx;
  color: blue;
  text-decoration: underline;
}

.shared{
  display: block;
  width: 100%;
  margin: 0 auto 0;
  position: fixed;
  z-index: 999;
  height: 100vh;
  text-align: center;
  background-color: rgba(0, 0, 0, 0.6)
}
.shared image{
  padding-top: 50rpx;
  display: block;
  margin: 0 auto;
  width: 80%;
  height: 800rpx;
}
.shared .fenxiang{
  width: 70%;
  background-color: #50a5fc;
  border-radius: 10px;
